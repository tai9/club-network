{
  "name": "club-network-server",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "engines": {
    "node": "18.x"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "node -r tsconfig-paths/register -r ts-node/register ./src/index.ts",
    "ext": "ts, js"
  },
  "scripts": {
    "dev": "nodemon",
    "build-ts": "tsc",
    "build": "yarn run build-ts",
    "start": "nodemon dist/index.js",
    "typeorm": "typeorm-ts-node-commonjs -d ./src/configs/db.config.ts",
    "migration-generate": "typeorm migration:create",
    "migration:up": "yarn run typeorm migration:run",
    "migration:down": "yarn run typeorm migration:revert",
    "entity:create": "typeorm entity:create"
  },
  "author": "tailor",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.4",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.9",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.8.3",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@thirdweb-dev/sdk": "^4",
    "@types/papaparse": "^5.3.14",
    "axios": "^1.6.7",
    "bcrypt": "^5.1.1",
    "connect-redis": "^7.1.0",
    "cookie-parser": "^1.4.6",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "ethers": "^5",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "logform": "^2.5.1",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "next-auth": "^4.24.7",
    "papaparse": "^5.4.1",
    "pg": "^8.11.3",
    "socket.io": "^4.7.2",
    "typeorm": "^0.3.20",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  }
}
